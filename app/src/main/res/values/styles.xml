<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/background_light</item>
        <item name="colorPrimaryDark">@android:color/darker_gray</item>
        <item name="colorAccent">@android:color/primary_text_light</item>
        <item name="android:navigationBarColor">?android:attr/textColorTertiary</item>
        <item name="android:statusBarColor">?android:attr/textColorTertiary</item>
    </style>

    <style name="p" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="colorAccent">@color/accent_material_light_1</item>
    </style>

    <style name="o" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="android:statusBarColor">?android:attr/colorControlActivated</item>
    </style>

</resources>
